    "import os\n",
    "\n",
    "\n",
    "def print_answer(num: int, text: str) -> None:\n",
    "    if not os.path.exists(\"answers\"):\n",
    "        os.makedirs(\"answers\")\n",
    "\n",
    "    with open(os.path.join(\"answers\", f\"a{num}.txt\"), \"w\") as f:\n",
    "        f.write(text)\n",
    "\n",
    "    print(text)"
